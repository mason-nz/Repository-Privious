<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <appSettings>
    <add key="Sys_ConnectionStrings" value="server=192.168.15.62\develop;uid=sa;pwd=sa;database=SysRightsManager;Max Pool Size=300;"/>
    <add key="ConnectionStrings_CC" value="server=192.168.15.62\develop;uid=sa;pwd=sa;database=CC2012;Max Pool Size=300;"/>
    <add key="ConnectionStrings" value="server=192.168.15.62\develop;uid=sa;pwd=sa;database=CRM2009;Max Pool Size=300;"/>
    <!--<add key="ConnectionStrings_CRM" value="server=192.168.15.62\develop;uid=sa;pwd=sa;database=CRM2009;Max Pool Size=300;"/>-->
    <!--系统编号-->
    <add key="ThisSysID" value="SYS024"/>
    <!--退出系统地址-->
    <add key="ExitAddress" value="http://ncc.sys1.bitauto.com"/>
    <!--提示“没有权限访问”的页面地址-->
    <add key="NotAccessMsgPagePath" value="~/ErrorPage/NotAccessMsgPage.aspx"/>
    <!-- 外网正式服务地址 -->
    <add key="OrganizationService" value="tcp://59.151.102.27:20007/OrganizationService"/>
    <!-- 系统登录操作日志模块ID -->
    <add key="LoginLogModuleID" value="LM024001"/>
    <!--发送邮件模板-->
    <add key="MailTemplatePath" value="E:\project\A5信息系统研发\销售业务管理平台\客户关系管理\BitAuto.ISDC.CC2012.Web\MailTemplat"/>
    <add key="MailServerAddress" value="mail.hd.bitauto.com"/>
    <add key="MailSendAddress" value="isdc@bitauto.com"/>
    <add key="MailSendPassword" value="@WSX2wsx2012"/>
    <!--发送邮件到任务处理人地址-->
    <add key="TaskProcessUrl" value="http://ncc.sys1.bitauto.com"/>
  </appSettings>
  <connectionStrings/>
  <log4net>
    <root>
      <level value="All"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="log\SendMail.Log"/>
      <param name="AppendToFile" value="true"/>
      <param name="MaxSizeRollBackups" value="10"/>
      <param name="MaximumFileSize" value="10MB"/>
      <param name="RollingStyle" value="Size"/>
      <param name="StaticLogFileName" value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] - %m%n"/>
      </layout>
    </appender>
  </log4net>
  <system.web>
    <compilation debug="true">
      <assemblies>
        <add assembly="System.Core, Version=3.5.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
        <add assembly="System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
        <add assembly="System.Xml.Linq, Version=3.5.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
        <add assembly="System.Data.DataSetExtensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
      </assemblies>
    </compilation>
    <!--
            通过 <authentication> 节可以配置
            安全身份验证模式，ASP.NET 
            使用该模式来识别来访用户身份。 
        -->
    <authentication mode="Forms">
      <forms loginUrl="Login.aspx" timeout="40" name="ExtTaskSessionID.ASPXAUTH"></forms>
    </authentication>
    <authorization>
      <deny users="?"/>
    </authorization>


    <!--
            如果在执行请求的过程中出现未处理的错误，
            则通过 <customErrors> 节
            可以配置相应的处理步骤。具体而言，
            开发人员通过该节可配置要显示的 html 错误页，
            以代替错误堆栈跟踪。

        <customErrors mode="RemoteOnly" defaultRedirect="GenericErrorPage.htm">
            <error statusCode="403" redirect="NoAccess.htm" />
            <error statusCode="404" redirect="FileNotFound.htm" />
        </customErrors>
        -->
    <customErrors mode="Off"></customErrors>
    
    <pages>
      <controls>
        <add tagPrefix="uc" src="~/CustBaseInfo/UCCustBaseInfo/EditCustBaseInfo.ascx" tagName="CustBaseInfo"/>
        <add tagPrefix="asp" namespace="System.Web.UI" assembly="System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
        <add tagPrefix="asp" namespace="System.Web.UI.WebControls" assembly="System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
      </controls>
    </pages>
    <httpHandlers>
      <remove verb="*" path="*.asmx"/>
      <add verb="*" path="*.asmx" validate="false" type="System.Web.Script.Services.ScriptHandlerFactory, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
      <add verb="*" path="*_AppService.axd" validate="false" type="System.Web.Script.Services.ScriptHandlerFactory, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
      <add verb="GET,HEAD" path="ScriptResource.axd" validate="false" type="System.Web.Handlers.ScriptResourceHandler, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
    </httpHandlers>
    <httpModules>
      <add name="ScriptModule" type="System.Web.Handlers.ScriptModule, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
    </httpModules>
  </system.web>
  
  <location path="Js">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="Css">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="Images">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="AjaxServers">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
  
</configuration>