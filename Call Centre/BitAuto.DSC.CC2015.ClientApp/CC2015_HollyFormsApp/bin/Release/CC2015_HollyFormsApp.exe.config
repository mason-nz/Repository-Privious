<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <appSettings>
    <!--系统编号-->
    <add key="ThisSysID" value="SYS024"/>
    <add key="OrganizationService" value="tcp://59.151.102.195:20007/OrganizationService"/>
    <!--测试登录页面-->
    <add key="IsTestLogin" value="true"/>
    <!--调用话务记录接口授权码-->
    <add key="CallRecordAuthorizeCode" value="B8C9EC64-0B98-4C59-B423-C68B0854E3A6"/>
    <!--客户端登陆后，默认访问页面URL-->
    <add key="DefaultURL" value="http://ncc.sys1.bitauto.com"/>

    <!--录音地址-->
    <add key="RecordURl" value="http://192.168.112.208"/>
    <!--满意度标志-->
    <add key="IVRFlowFlag" value="4"/>
    <!--满意度号码(生产环境，不需要配置)-->
    <add key="IVRDestNo" value="58103008"/>
    <!--本地区域 （不是本地区域的电话号码要加0）-->
    <add key="LocalRegion" value="西安"/>
    <!--话后最长时间 秒-->
    <add key="AfterTime" value="59"/>
    <!--坐席登录的分机与SIP_Server对应关系 正式：31001-31200|192.168.112.201,32001-32200|192.168.112.202,33001-33200|192.168.112.203  测试：3001-3010|192.168.15.84-->
    <add key="DN_SIPServerIP_Mapping" value="31001-31200|192.168.112.201,32001-32200|192.168.112.202,33001-33200|192.168.112.203,34001-34200|192.168.112.204,35001-35200|192.168.112.205,36001-37000|192.168.112.200"/>
    <!--默认Softphone.ini位置 测试用（E:\CC2015\UniSoftPhone2.1(mini)）-->
    <add key="DefaultSoftphoneINI" value="E:\CC2015\UniSoftPhone2.1(mini)"/>
    <!--调用合力厂家CTI-SDK接口Load异步函数，回调接受消息的个数-->
    <add key="LoadConfigCount" value="6"/>

    <!--发送邮件模板-->
    <add key="MailTemplatePath" value="MailTemplat"/>
    <add key="MailServerAddress" value="mail.hd.bitauto.com"/>
    <add key="MailSendAddress" value="isdc@bitauto.com"/>
    <add key="MailSendPassword" value="@WSX2wsx2012"/>
    <!--系统报错邮件接收者masj@bitauto.com;liuxuewen@bitauto.com;qizq@bitauto.com;yangyh@bitauto.com;weisz@bitauto.com;liuping@bitauto.com-->
    <add key="ReceiveErrorEmail" value="qiangfei@yiche.com;masj@yiche.com;liuping@yiche.com;zhangyuan5@yiche.com;weisz@yiche.com"/>
  </appSettings>
  <log4net>
    <root>
      <level value="All"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="log\log_" />
      <param name="AppendToFile" value="true" />
      <param name="MaxSizeRollBackups" value="10" />
      <param name="MaximumFileSize" value="5MB" />
      <param name="RollingStyle" value="Composite" />
      <datePattern value="yyyyMMdd'.log'"/>
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] - %m%n" />
      </layout>
    </appender>
  </log4net>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="VerifyPhoneFormatSoap" />
        <binding name="AgentTimeStateSoap" />
        <binding name="CallRecordServiceSoap" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://ncc.sys1.bitauto.com/VerifyPhoneFormat.asmx"
        binding="basicHttpBinding" bindingConfiguration="VerifyPhoneFormatSoap"
        contract="CCWebVerifyPhoneFormat.VerifyPhoneFormatSoap" name="VerifyPhoneFormatSoap" />
      <endpoint address="http://ncc.sys1.bitauto.com/CallRecordService/CallRecordService.asmx"
        binding="basicHttpBinding" bindingConfiguration="CallRecordServiceSoap"
        contract="CCWeb.CallRecordService.CallRecordServiceSoap" name="CallRecordServiceSoap" />
      <endpoint address="http://ncc.sys1.bitauto.com/CallRecordService/AgentTimeState.asmx"
        binding="basicHttpBinding" bindingConfiguration="AgentTimeStateSoap"
        contract="CCWebAgentTimeState.AgentTimeStateSoap" name="AgentTimeStateSoap" />
    </client>
  </system.serviceModel>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri=""/>
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400"/>
      </providers>
    </roleManager>
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NPOI" publicKeyToken="0DF73EC7942B34E1" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-2.0.1.0" newVersion="2.0.1.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
