    <add key="TimeidleSendMessage" value="480" />
    <!--网友空闲时间,也就是没有长连接多久网友对象将被清除(秒)-->
    <add key="TimeidleUser" value="60" />
    <!--坐席空闲时间,也就是没有长连接多久坐席对象将被清除(秒)-->
    <add key="TimeidleAgent" value="60" />
    <!--聊天记录多久插入一次数据库，单位秒(秒)-->
    <add key="TimeInDB" value="10" />
    <!--客服最大同时排队量-->
    <add key="MaxQueue" value="30" />
    <!--客服最大同时会话量-->
    <add key="MaxDialogueN" value="10" />
    <!--系统编号-->
    <add key="ThisSysID" value="SYS034" />
    <!-- 外网正式服务地址 -->
    <add key="OrganizationService" value="tcp://59.151.102.27:20007/OrganizationService" />
    <!--提示“没有权限访问”的页面地址-->
    <add key="NotAccessMsgPagePath" value="~/ErrorPage/NotAccessMsgPage.aspx" />
    <!-- 系统登录操作日志模块ID -->
