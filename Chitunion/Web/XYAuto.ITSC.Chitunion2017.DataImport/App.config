<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <appSettings>
    <!--<add key="ConnectionStrings_ITSC" value="server=192.168.3.51\develop,52436;uid=sa;pwd=sa;database=Chitunion2017;Max Pool Size=300;Application Name=media_test" />-->
    <add key="ConnectionStrings_ITSC" value="server=192.168.3.152\DEV,4000;uid=sa;pwd=123.abc;database=Chitunion2017;Max Pool Size=300;Application Name=media_test" />
    <add key="ExcelPath" value="app0520.xls" />
    <add key="ImportUserID" value="1192" />
    <add key="UserSource" value="3001" />
    <add key="DefaultPubEndDate" value="2017-06-30" />
    <!--<add key="ImportItem" value="微信号,微博,视频,直播,APP" />-->
    <add key="ImportItem" value="APP" />
    <!--APP媒体下模板、刊例、广告数据Excel-->
    <add key="APP_ExcelPath" value="Import_files\app模板刊例导入.xls" />
    <!--开发环境：运营，AE-->
    <!--<add key="APP_ImportTempleteUserID" value="1225"/>
    <add key="APP_ImportPublishUserID" value="1192"/>-->
    <!--测试环境：运营13810184685，AE13426262451-->
    <add key="APP_ImportTempleteUserID" value="2" />
    <add key="APP_ImportPublishUserID" value="4" />
    <!--模板价格导入：是否先删后插，1：是，其它值：否-->
    <add key="APP_isDelete" value="1" />
    <!--验证登陆时，MD5加密key-->
    <add key="LoginPwdKey" value="1234567890ABC" />
    <!--登陆后，写入Cookies作用域-->
    <add key="WebDomain" value="chitunion.com" />
  </appSettings>
  <log4net>
    <appender name="ColoredConsoleAppender" type="log4net.Appender.ColoredConsoleAppender">
      <mapping>
        <level value="ERROR" />
        <foreColor value="Red" />
      </mapping>
      <mapping>
        <level value="DEBUG" />
        <foreColor value="Yellow" />
      </mapping>

      <mapping>
        <level value="Info" />
        <foreColor value="White" />
      </mapping>

      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date- %message%newline" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <param name="LevelMin" value="Debug" />
        <param name="LevelMax" value="Fatal" />
      </filter>
    </appender>

    <root>
      <level value="ALL" />
      <appender-ref ref="ColoredConsoleAppender" />
    </root>

    <logger>
      <level value="ALL" />
    </logger>
  </log4net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NPOI" publicKeyToken="0df73ec7942b34e1" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.0.1.0" newVersion="2.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" /></startup></configuration>
