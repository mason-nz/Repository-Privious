    <!-- 高级接口信息 -->
    <add key="WeixinAppId" value="wxf0eea6fec2756b45" />
    <add key="WeixinAppSecret" value="2e35c3d7acb3219b6d77415267d06975" />
    <!--上传文件存放物理路径-->
    <add key="UploadFilePath" value="D:\wwwroot\XYAuto.ITSC.Chitunion2017.Upload"/>
    <!--允许上传的文件头信息(7790是exe,8297是rar)-->
    <add key="AllowUploadFileHeaderInfo" value="jpg:255216,png:13780,bmp:6677" />
    <!--可访问临时图片用于图片上传CDN-->
    <add key="Domin" value="http://wxtest-ct.qichedaquan.com" />
