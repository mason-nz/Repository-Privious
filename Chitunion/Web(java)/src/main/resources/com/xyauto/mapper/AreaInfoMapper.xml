<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xyauto.mapper.AreaInfoMapper">
	<select id="getAreaInfoList" parameterType="java.lang.String"
		resultType="com.xyauto.domain.AreaInfo">
		SELECT
		area.AreaID,area.AbbrName,area.PID,area.AreaName,area.CreateTime,area.Level
		FROM AreaInfo area
	</select>
</mapper>